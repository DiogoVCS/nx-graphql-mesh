{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "title": "Build executor",
  "description": "",
  "type": "object",
  "properties": {
    "meshYmlPath": {
      "description": "Path to where the .meshrc.yml file is located",
      "type": "string"
    },
    "fileType": {
      "description": "Type of file: [choices: 'json', 'ts'] [default: 'ts']",
      "type": "string",
      "pattern": "ts|json",
      "default": "ts"
    }
  },
  "required": [
    "meshYmlPath"
  ]
}
